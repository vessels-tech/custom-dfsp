{
  "name": "sheet-fsp",
  "version": "0.1.5",
  "description": "A custom DFSP implementation that uses the scheme adapter to talk to Mojaloop switches/peers.",
  "main": "./dist/index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "start": "node ./dist",
    "watch": "TSC_NONPOLLING_WATCHER='1' tsc-watch --onSuccess \"node ./dist\"",
    "watch-ts-node": "ts-node-dev --poll --interval 1000 src/index.ts",
    "dev": "nodemon ./dist",
    "raml": "raml2html -i doc/api/api.raml -o api.html",
    "knex": "knex --knexfile ./config/knexfile.js"
  },
  "engines": {
    "node": "10.x"
  },
  "dependencies": {
    "@hapi/boom": "7.4.2",
    "@hapi/joi": "15.1.0",
    "@modusbox/mojaloop-sdk-standard-components": "^0.0.39",
    "knex": "0.19.5",
    "koa": "2.11.0",
    "koa-404-handler": "0.0.2",
    "koa-better-error-handler": "2.0.0",
    "koa-body": "2.5.0",
    "koa-convert": "1.2.0",
    "koa-helmet": "4.0.0",
    "koa-joi-validate": "0.5.1",
    "koa-morgan": "1.0.1",
    "koa-openapi": "3.13.5",
    "koa-router": "7.4.0",
    "mongodb": "^3.3.2",
    "pg": "7.11.0",
    "rc": "1.2.8",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "request-promise-native": "^1.0.7",
    "tslib": "1.10.0",
    "twilio": "3.33.1",
    "uuid": "^3.3.3",
    "winston": "3.2.1"
  },
  "devDependencies": {
    "@types/hapi__boom": "7.4.0",
    "@types/hapi__joi": "^15.0.3",
    "@types/koa": "2.0.52",
    "@types/koa-convert": "1.2.2",
    "@types/koa-helmet": "3.1.2",
    "@types/koa-morgan": "1.0.4",
    "@types/koa-router": "7.0.42",
    "@types/mongodb": "^3.3.1",
    "@types/rc": "1.1.0",
    "@types/request-promise-native": "^1.0.16",
    "@types/twilio": "0.0.10",
    "@types/winston": "2.4.4",
    "@typescript-eslint/eslint-plugin": "1.11.0",
    "@typescript-eslint/parser": "1.11.0",
    "eslint": "6.0.1",
    "eslint-config-prettier": "6.0.0",
    "eslint-plugin-prettier": "3.1.0",
    "nodemon": "1.19.1",
    "raml2html": "7.1.0",
    "ts-node": "8.3.0",
    "ts-node-dev": "^1.0.0-pre.42",
    "tsc-watch": "2.2.1",
    "typescript": "3.5.3"
  }
}
